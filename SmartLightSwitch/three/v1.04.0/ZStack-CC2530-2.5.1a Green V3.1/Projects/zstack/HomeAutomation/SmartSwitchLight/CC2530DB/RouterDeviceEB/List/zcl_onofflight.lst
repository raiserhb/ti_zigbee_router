    626          /*********************************************************************
    627           * @fn      zclOnOffLight_ProcessIncomingMsg
    628           *
    629           * @brief   Process ZCL Foundation incoming message
    630           *
    631           * @param   pInMsg - pointer to the received message
    632           *
    633           * @return  none
    634           */
    635          static void zclOnOffLight_ProcessIncomingMsg( zclIncomingMsg_t *pInMsg)
    636          {
    637           
    638            switch ( pInMsg->zclHdr.commandID )
    639            {
    640          #ifdef ZCL_READ
    641              case ZCL_CMD_READ_RSP:
    642                zclOnOffLight_ProcessInReadRspCmd( pInMsg );
    643                break;
    644          #endif
    645          #ifdef ZCL_WRITE    
    646              case ZCL_CMD_WRITE_RSP:
    647                zclOnOffLight_ProcessInWriteRspCmd( pInMsg );
    648                break;
    649          #endif
    650          #ifdef ZCL_REPORT
    651              // See ZCL Test Applicaiton (zcl_testapp.c) for sample code on Attribute Reporting
    652              case ZCL_CMD_CONFIG_REPORT:
    653                //zclOnOffLight_ProcessInConfigReportCmd( pInMsg );
    654                break;
    655              
    656              case ZCL_CMD_CONFIG_REPORT_RSP:
    657                //zclOnOffLight_ProcessInConfigReportRspCmd( pInMsg );
    658                break;
    659              
    660              case ZCL_CMD_READ_REPORT_CFG:
    661                //zclOnOffLight_ProcessInReadReportCfgCmd( pInMsg );
    662                break;
    663              
    664              case ZCL_CMD_READ_REPORT_CFG_RSP:
    665                //zclOnOffLight_ProcessInReadReportCfgRspCmd( pInMsg );
    666                break;
    667              
    668              case ZCL_CMD_REPORT:
    669                //zclOnOffLight_ProcessInReportCmd( pInMsg );
    670                break;
    671          #endif   
    672              case ZCL_CMD_DEFAULT_RSP:
    673                zclOnOffLight_ProcessInDefaultRspCmd( pInMsg );
    674                break;
    675          #ifdef ZCL_DISCOVER     
    676              case ZCL_CMD_DISCOVER_RSP:
    677                zclOnOffLight_ProcessInDiscRspCmd( pInMsg );
    678                break;
    679          #endif  
    680              default:
    681                break;
    682            }
    683            
    684            if ( pInMsg->attrCmd )
    685              osal_mem_free( pInMsg->attrCmd );
    686          }
